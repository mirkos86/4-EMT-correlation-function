Cell[TextData[{
 "In the notation in this section, we distinguish between covariant metrics ",
 Cell[BoxData[
  FormBox[
   RowBox[{"(", 
    SubscriptBox["g", "\[Mu]\[Nu]"]}], TraditionalForm]],ExpressionUUID->
  "9754bc45-c0da-4b60-b8ae-792b0bd7d665"],
 ") and contravariant metrics ",
 Cell[BoxData[
  FormBox[
   RowBox[{"(", 
    SuperscriptBox["g", "\[Mu]\[Nu]"]}], TraditionalForm]],ExpressionUUID->
  "b36d9495-2afb-4562-b6fc-f20acee274a6"],
 ") in the names of the functional derivatives, using normal (g) and capital \
(G) letters respectively. \[LineSeparator]As explained above, the number at \
the end of the name stands for the number of functional derivatives. "
}], "Text",
 CellChangeTimes->{
  3.795403647248098*^9, {3.795403757369876*^9, 3.795403785086218*^9}, {
   3.795404812131517*^9, 3.7954048521568327`*^9}, 
   3.795404983771516*^9},ExpressionUUID->"cd576201-04eb-4412-b274-\
778c929004e0"],

Cell[CellGroupData[{

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   FractionBox[
    RowBox[{"\[Delta]", " ", 
     SubscriptBox["g", 
      RowBox[{"m", ",", "n"}]]}], 
    RowBox[{"\[Delta]", " ", 
     SubscriptBox["g", 
      RowBox[{"\[Mu]2", ",", "\[Nu]2"}]]}]], TraditionalForm]],ExpressionUUID->
  "6a681311-71bb-401b-b345-c2934a23564a"],
 " = \[Integral] ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["dx", "1"], 
    SubscriptBox["dx", "2"], 
    SuperscriptBox["e", 
     RowBox[{"i", " ", 
      RowBox[{"(", 
       RowBox[{
        RowBox[{
         SubscriptBox["x", "1"], ".", 
         SubscriptBox["k", "1"]}], "-", 
        RowBox[{
         SubscriptBox["x", "2"], ".", 
         SubscriptBox["k", "2"]}]}], ")"}], " "}]], 
    FractionBox[
     RowBox[{"\[Delta]", " ", 
      RowBox[{
       SubscriptBox["g", "mn"], "(", 
       SubscriptBox["x", "1"], ")"}]}], 
     RowBox[{
      SubscriptBox["\[Delta]g", "\[Mu]2\[Nu]2"], "(", 
      SubscriptBox["x", "2"], ")"}]]}], TraditionalForm]],ExpressionUUID->
  "801e07d5-d43e-4a88-a626-41b6b3990a53"]
}], "Subsubsection",
 CellChangeTimes->{{3.795341504061103*^9, 3.795341541925515*^9}, {
  3.7953416612467623`*^9, 3.7953416644123507`*^9}, {3.795405035111642*^9, 
  3.7954050612957582`*^9}, {3.7954056388812437`*^9, 
  3.79540564000276*^9}},ExpressionUUID->"953d24a1-e56a-4118-ae64-\
bb9088d30c2c"],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"g1", "[", 
    RowBox[{"\[Mu]2_", ",", "\[Nu]2_", ",", "m_", ",", "n_"}], "]"}], "=", 
   RowBox[{
    FractionBox["1", "2"], 
    RowBox[{"(", 
     RowBox[{
