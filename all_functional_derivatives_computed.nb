     "loop", " ", "energy", " ", "momentum", " ", "tensor", " ", "is"}]}], 
    " ", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{
      RowBox[{"-", 
       FractionBox[
        RowBox[{
         SuperscriptBox["\[Mu]", 
          RowBox[{"-", "\[Epsilon]"}]], " "}], "\[Epsilon]"]}], 
      RowBox[{"\[Integral]", 
       RowBox[{
        SuperscriptBox["d", "d"], "x", " ", 
        RowBox[{"(", 
         RowBox[{
          RowBox[{
           SubscriptBox["\[Beta]", "a"], "F"}], " ", "+", " ", 
          RowBox[{
           SubscriptBox["\[Beta]", "b"], "G"}]}], ")"}]}]}]}], ",", " ", 
     RowBox[{
     "where", " ", "F", " ", "and", " ", "G", " ", "were", " ", "given", " ", 
      RowBox[{"above", ".", " ", "\[LineSeparator]", "They"}], " ", "are", 
      " ", "both", " ", "linear", " ", "combinations", " ", "of", " ", 
      "terms", " ", "quadratic", " ", "in", " ", "the", " ", "Rieman", " ", 
      "and", " ", "Ricci", " ", "tensors"}], ",", 
     RowBox[{
      RowBox[{"i", ".", "e", ".", " ", "\[LineSeparator]", "K"}], " ", "=", 
      "  ", 
      RowBox[{
       RowBox[{"a", " ", 
        SuperscriptBox["R", "\[Alpha]\[Beta]\[Gamma]\[Delta]"], 
        SubscriptBox["R", "\[Alpha]\[Beta]\[Gamma]\[Delta]"]}], " ", "+", " ", 
       RowBox[{"b", " ", 
        SuperscriptBox["R", "\[Alpha]\[Beta]"], 
        SubscriptBox["R", "\[Alpha]\[Beta]"]}], " ", "+", " ", 
       RowBox[{"c", " ", 
        RowBox[{
         SuperscriptBox["R", "2"], ".", " ", "\[LineSeparator]", 
         "\[LineSeparator]", "It"}], " ", "turns", " ", "out", " ", "that", 
        " ", "a", " ", "general", " ", "formula", "  ", "for", " ", "the", 
        " ", "first", " ", "functional", " ", "derivaive", " ", "of", " ", 
        "such", " ", "an", " ", "integral", " ", "is"}]}]}]}], " ", 
    "\[LineSeparator]", 
    StyleBox[
     RowBox[{
      RowBox[{
       FractionBox["\[Delta]", 
