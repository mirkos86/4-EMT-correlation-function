Cell[CellGroupData[{

Cell[TextData[Cell[BoxData[
 FormBox[
  RowBox[{
   FractionBox[
    RowBox[{"\[Delta]", " ", 
     SubscriptBox["g", 
      RowBox[{"a", ",", "b"}]], " ", 
     SubscriptBox["g", 
      RowBox[{"r", ",", "s"}]]}], 
    RowBox[{"\[Delta]", " ", 
     SubscriptBox["g", "\[Mu]2\[Nu]2"]}]], "=", " ", 
   RowBox[{"\[Integral]", 
    FormBox[
     RowBox[{
      SubscriptBox["dx", "1"], 
      SubscriptBox["dx", "2"], 
      SuperscriptBox["e", 
       RowBox[{"i", " ", 
        RowBox[{"(", 
         RowBox[{
          RowBox[{
           SubscriptBox["x", "1"], ".", 
           SubscriptBox["k", "1"]}], "-", 
          RowBox[{
           SubscriptBox["x", "2"], ".", 
           SubscriptBox["k", "2"]}]}], ")"}], " "}]], 
      FractionBox[
       RowBox[{"\[Delta]", " ", 
        RowBox[{
         SubscriptBox["g", "mn"], "(", 
         SubscriptBox["x", "1"], ")"}], 
        RowBox[{
         SubscriptBox["g", "rs"], "(", 
         SubscriptBox["x", "1"], ")"}]}], 
       RowBox[{
        SubscriptBox["\[Delta]g", "\[Mu]2\[Nu]2"], "(", 
        SubscriptBox["x", "2"], ")"}]]}],
     TraditionalForm]}]}], 
  TraditionalForm]],ExpressionUUID->"72daa758-619f-4242-8a0c-6f85dba7cb23"]], \
"Subsubsection",
 CellChangeTimes->{{3.795341618197637*^9, 3.795341668440525*^9}, {
  3.7954054009256887`*^9, 3.795405416062244*^9}, {3.795405636381638*^9, 
  3.795405637346573*^9}},ExpressionUUID->"af77d029-3d93-41d6-bc75-\
37e6edf59b93"],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"gg1", "[", 
    RowBox[{
    "\[Mu]2_", ",", "\[Nu]2_", ",", "a_", ",", "b_", ",", "r_", ",", "s_"}], 
    "]"}], "=", 
   RowBox[{
    RowBox[{
     SubscriptBox["\[DoubleStruckG]", 
